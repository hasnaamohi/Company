@model Department
@{
    ViewData["Title"] = "Details";
}

<h1>Details page</h1>
 <br />
 <br />
 <div asp-action="Details">
     <div class="m-2">
     <label asp-for="Code" class="col-form-lable" >Code</label>
     <input asp-for="Code" class="form-control" readonly />
    
     </div>

     <div class="m-2">
         <label asp-for="Name" class="col-form-label" >Name</label>
         <input asp-for="Name" class="form-control" readonly/>
         

     </div>
     <div class="m-2">
         <label asp-for="CreateAt" class="col-form-label">Date Of Creation</label>
         <input asp-for="CreateAt" class="form-control" readonly />
         

     </div>
     <br />
     <br />
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-warning">Edit</a>
     <a asp-action="Index" class="btn btn-light">Back to list</a>
 </div>
