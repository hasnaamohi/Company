
@model DepartmentDto
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit Department </h1>
<br />
<br />
<form class="Edit">
    <div class="m-2">
        <label asp-for="Code" class=" col-form-label">Code</label>
        <input asp-for="Code" class="form-control"/>
        <span asp-validation-for="Code" class="text-danger"></span>
    </div>

    <div class="m-2">
        <label asp-for="Name" class="col-form-label"> </label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="m-2">
        <label asp-for="CreateAt" class="col-form-label"> </label>
        <input asp-for="CreateAt" class="form-control" />
        <span asp-validation-for="CreateAt" class="text-danger"></span>
    </div>
    <div class="m-2">

        <input type="submit" value="Save" class=" btn btn-warning " />
        <a asp-action="index" class="btn btn-primary"> Back to list</a>

    </div>
</form>

@section VailtionSection {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}