@model Employee
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit Page</h1>
<br />

<br />
<form asp-action="Edit">
    <div class="m-3">
        <label asp-for="Name" class="col-form-label"> </label>
        <input asp-for="Name" class="form-control" />

    </div>
    <div class="m-3">
        <label asp-for="Age" class="col-form-label"> </label>
        <input asp-for="Age" class="form-control" />

    </div>

    <div class="m-3">
        <label asp-for="Email" class="col-form-label"> </label>
        <input asp-for="Email" class="form-control" />

    </div>
    <div class="m-3">
        <label asp-for="Phone" class="col-form-label"> </label>
        <input asp-for="Phone" class="form-control"  />

    </div>
    <div class="m-3">
        <label asp-for="Address" class="col-form-label"> </label>
        <input asp-for="Address" class="form-control" readonly />

    </div>
    <div class="m-3">
        <label asp-for="Salary" class="col-form-label"> </label>
        <input asp-for="Salary" class="form-control" readonly />

    </div>

    <div class="m-3">
        <input asp-for="IsDeleted" class="col-form-label" />
        <label asp-for="IsDeleted" class="form-check" readonly> </label>


    </div>
    <div class="m-3">

        <input asp-for="IsActive" class="col-form-label" />
        <label asp-for="IsActive" class="form-check" disabled> </label>

    </div>
    <div class="m-3">
        <label asp-for="HiringDate" class="col-form-label"> </label>
        <input asp-for="HiringDate" class="form-control"  />

    </div>
    <div class="m-3">
        <label asp-for="CreateAt" class="col-form-label"> </label>
        <input asp-for="CreateAt" class="form-control"  />

    </div>
    <div class="m-3">

        <input type="submit" value="Save" class=" btn btn-primary " />
        <a asp-action="index" class="btn btn-primary"> Back to list</a>

    </div>
</form>


